{
  "name": "@edx/frontend-lib-learning-assistant",
  "version": "1.0.0-semantically-released",
  "description": "A frontend library for an Open edX learning assistant.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-lib-learning-assistant.git"
  },
  "browserslist": [
    "extends @edx/browserslist-config"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "build": "make build",
    "i18n_extract": "fedx-scripts formatjs extract",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --fix --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "files": [
    "/dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/edx/frontend-lib-learning-assistant#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/edx/frontend-lib-learning-assistant/issues"
  },
  "dependencies": {
    "@edx/brand": "npm:@edx/brand-openedx@1.2.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@optimizely/react-sdk": "^2.9.2",
    "core-js": "3.31.1",
    "prop-types": "15.8.1",
    "react-markdown": "^8.0.5",
    "uuid": "9.0.0"
  },
  "peerDependencies": {
    "@edx/frontend-platform": "^4.3.0 || ^5.0.0",
    "@edx/paragon": "20.46.0",
    "@reduxjs/toolkit": "1.8.1",
    "react": "16.14.0 || ^17.0.0",
    "react-dom": "16.14.0 || ^17.0.0",
    "react-redux": "7.2.9",
    "react-router": "5.2.1 || ^6.0.0",
    "react-router-dom": "5.3.0 || ^6.0.0",
    "redux": "4.1.2",
    "regenerator-runtime": "0.13.11"
  },
  "devDependencies": {
    "@edx/browserslist-config": "^1.1.1",
    "@edx/frontend-build": "13.0.0",
    "@edx/frontend-platform": "5.0.0",
    "@edx/paragon": "20.46.0",
    "@edx/reactifex": "^2.1.1",
    "@reduxjs/toolkit": "1.8.1",
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "glob": "7.2.3",
    "husky": "7.0.4",
    "jest": "29.6.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "^5.25.0",
    "react-redux": "7.2.9",
    "react-router": "6.15.0",
    "react-router-dom": "6.15.0",
    "redux": "4.1.2",
    "regenerator-runtime": "0.13.11",
    "semantic-release": "^21.0.7"
  }
}
